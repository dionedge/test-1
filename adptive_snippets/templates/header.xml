<odoo>
<template id="layout" name="Header" inherit_id="theme_watch.layout">
<xpath expr="//div[hasclass('header-middle')]" position="replace">
</xpath>
<xpath expr="//div[hasclass('header-top')]" position="attributes">
	<attribute name="class">header-top header_top</attribute>
</xpath>
 <xpath expr="//div[hasclass('navbar-static-top')]" position="attributes">
	<attribute name="class">nav_bar</attribute>
</xpath> 
 <xpath expr="//div[hasclass('navbar-top-collapse')]" position="before">
<img src="/adptive_snippets/static/src/img/footer-logo.png" alt="D1 Sports" class="company_logo"/>
</xpath>  

</template>
</odoo>